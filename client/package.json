{
  "name": "bitcentivez-client",
  "description": "Client application.",
  "license": "MIT",
  "homepage": "bitcentivez.herokuapp.com",
  "repository": "https://github.com/leoj3n/bitcentivez",
  "version": "0.0.1",
  "main": "bitcentivez-client/index.stache!done-autorender",
  "scripts": {
    "build": "rm -rf ./prod && mkdir ./prod && node build && cp -r ./node_modules/events ./dist/node_modules && mv ./dist ./prod && cp ./package.json ./favicon.ico ./prod",
    "test": "testee test.html --browsers firefox --reporter Spec"
  },
  "dependencies": {
    "@feathersjs/feathers": "^3.1.7",
    "@feathersjs/rest-client": "^1.4.5",
    "@feathersjs/socketio-client": "^1.1.5",
    "bootstrap": "^3.3.7",
    "can": "^5.15.1",
    "can-connect": "*",
    "can-event-dom-enter": "*",
    "can-stache-route-helpers": "*",
    "done-autorender": "^2.4.1",
    "done-component": "^2.2.0",
    "done-css": "^3.0.2",
    "jquery": "^3.3.1",
    "rollup": "^0.66.6",
    "steal": "^2.0.0",
    "steal-conditional": "^1.1.1",
    "steal-less": "^1.3.2",
    "steal-socket.io": "^4.1.0",
    "steal-stache": "^4.1.2",
    "steal-tools": "^2.0.2"
  },
  "devDependencies": {
    "funcunit": "^3.5.0",
    "steal-qunit": "^1.0.2",
    "testee": "^0.8.1"
  },
  "steal": {
    "treeShaking": false,
    "envs": {
      "server-production": {
        "renderingBaseURL": "https://bitcentivez.herokuapp.com/dist"
      }
    },
    "plugins": [
      "done-css",
      "steal-less",
      "steal-stache",
      "done-component"
    ],
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register",
      "node_modules/steal-conditional/conditional"
    ]
  }
}
