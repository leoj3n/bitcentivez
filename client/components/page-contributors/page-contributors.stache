<can-import from="./page-contributors.less" />

<div class="container">
  <h2>Contributors</h2>
  {{#if contributors.isResolved}}
    <can-dynamic-import from="./contributors-table/">
      {{#if isResolved}}
        <contributors-table contributors:from="contributors.value" />
      {{/if}}
    </can-dynamic-import>
  {{/if}}
</div>
